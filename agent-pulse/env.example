# VAPI Configuration
VAPI_API_KEY=your_vapi_api_key_here
ASSISTANT_SECRET_TOKEN=your_assistant_secret_token_here
PHONE_NUMBER_ID=your_phone_number_id_here
SURVEY_ASSISTANT_ID=your_survey_assistant_id_here

# Database Configuration
DATABASE_URL=postgresql+asyncpg://username:password@localhost:5432/database_name

# Webhook Configuration (optional - defaults provided)
WEBHOOK_ENDPOINT=/api/vapi-webhook
CALL_REPORTS_DIR=call_reports

# Note: If you have a vapi_webhook URL from VAPI dashboard, you can add it here
# but it won't be used by this application - the webhook endpoint is configured above
# vapi_webhook=https://your-domain.com/api/vapi-webhook 




