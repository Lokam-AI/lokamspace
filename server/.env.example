# Database Configuration
# Using port 5433 to avoid conflicts with existing PostgreSQL installations
POSTGRES_HOST=<your_postgres_host>
POSTGRES_PORT=<your_postgres_port>
POSTGRES_USER=<your_postgres_user>
POSTGRES_PASSWORD=<your_postgres_password>
POSTGRES_DB=<your_postgres_db>

# Application Settings
ENV=<your_environment>
DEBUG=<your_debug_setting>
LOG_LEVEL=<your_log_level>

# Security
SECRET_KEY=<your_secret_key>
CORS_ORIGINS=<your_cors_origins>

# JWT Authentication
JWT_SECRET=<your_jwt_secret>
JWT_ALGORITHM=<your_jwt_algorithm>
JWT_EXPIRATION=<your_jwt_expiration>

# VAPI Settings
VAPI_API_KEY=<your_vapi_api_key>
VAPI_ASSISTANT_ID=<your_vapi_assistant_id>
VAPI_DEMO_ASSISTANT_ID=<your_vapi_demo_assistant_id>
VAPI_PHONE_NUMBER_ID=<your_vapi_phone_number_id>
VAPI_WEBHOOK_SECRET=<your_vapi_webhook_secret>
VAPI_WEBHOOK_URL=<your_vapi_webhook_url>

OPENAI_API_KEY=<your_openai_api_key>