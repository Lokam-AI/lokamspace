# Database Configuration
DB_HOST=<YOUR_DB_HOST>
DB_PORT=<YOUR_DB_PORT>
DB_USER=<YOUR_DB_USER>
DB_PASSWORD=<YOUR_DB_PASSWORD>
DB_NAME=<YOUR_DB_NAME>

# Application Settings
ENV=<development|production>
DEBUG=<true|false>
LOG_LEVEL=<LOG_LEVEL>

# Security
SECRET_KEY=<SECRET_KEY>
CORS_ORIGINS=<ALLOWED_ORIGINS> # e.g., http://localhost:3000,http://localhost:5173

# JWT Authentication
JWT_SECRET=<JWT_SECRET>
JWT_ALGORITHM=<JWT_ALGORITHM> # e.g., HS256
JWT_EXPIRATION=<JWT_EXPIRATION_IN_SECONDS> # e.g., 86400

# VAPI Settings
VAPI_API_KEY=<VAPI_API_KEY>
VAPI_ASSISTANT_ID=<VAPI_ASSISTANT_ID>
VAPI_DEMO_ASSISTANT_ID=<VAPI_DEMO_ASSISTANT_ID>
VAPI_PHONE_NUMBER_ID=<VAPI_PHONE_NUMBER_ID>
VAPI_WEBHOOK_SECRET=<VAPI_WEBHOOK_SECRET>
VAPI_WEBHOOK_URL=<VAPI_WEBHOOK_URL>

# OpenAI Settings
OPENAI_API_KEY=<OPENAI_API_KEY>

# AnyLLM Additional Providers
# For Anthropic Claude models
ANTHROPIC_API_KEY=<ANTHROPIC_API_KEY>

# For Mistral AI models
MISTRAL_API_KEY=<MISTRAL_API_KEY>

# For Groq models
GROQ_API_KEY=<GROQ_API_KEY>

# For Ollama (local models)
OLLAMA_API_BASE=<OLLAMA_API_BASE> # Optional: defaults to http://localhost:11434
